
type User {
  id: ID! @unique
  auth0id: String @unique
  todoes: [ToDo!]!
  labels: [Label!]!
}

type ToDo {
  id: ID! @unique
  title: String
  description: String
  isDone: Boolean! @default(value: "false")
  user: User!
}

type Label {
  id: ID! @unique
  name: String
}

type Color {
  id: ID! @unique
  name: String
  colorCode: String
}
